{
  "name": "physical-education",
  "version": "1.0.0",
  "description": "Health Studio for Physical Education",
  "main": "src/app.ts",
  "repository": "git@github.com:Health-Studio/Physical-Education.git",
  "author": "Guilherme <guipython2001@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "lint": "eslint ./src ./test --ext .ts",
    "lint:fix": "eslint ./src ./test --ext .ts --fix",
    "style:check": "prettier --check 'src/**/*.ts' 'test/**/*.ts'",
    "style:fix": "prettier --write 'src/**/*.ts' 'test/**/*.ts'",
    "test:functional": "jest --projects ./test --runInBand",
    "test:unit": "jest --projects ./src",
    "test": "yarn lint && yarn style:check && jest --projects ./src ./test --runInBand --collectCoverage --silent",
    "db:migrations": "yarn prisma migrate deploy",
    "build": "tsc",
    "start": "LOG=true node ./dist/src/app.js",
    "deploy": "yarn install && yarn db:migrations && yarn build",
    "start:dev": "LOG=true ts-node -r tsconfig-paths/register ./src/app.ts"
  },
  "dependencies": {
    "@overnightjs/core": "^1.7.6",
    "@prisma/client": "4.9.0",
    "body-parser": "^1.20.1",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "http-status-codes": "^2.2.0",
    "module-alias": "^2.2.2",
    "pino": "^8.8.0",
    "pino-pretty": "^9.1.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.16",
    "@types/http-status-codes": "^1.2.0",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.11.18",
    "@types/pino": "^7.0.5",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "eslint": "^8.33.0",
    "jest": "^29.4.1",
    "jest-each": "^29.4.1",
    "prettier": "^2.8.3",
    "prisma": "^4.9.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.9.4"
  },
  "_moduleAliases": {
    "@src": "dist/src",
    "@test": "dist/test"
  }
}
